package practice;

public class User {
	private String id;
	private String pwd;
	private String name;
	
	public User(String id, String pwd, String name) {
		this.id = id;
		this.pwd = pwd;
		this.name = name;
	}
	
	
	public void setId(String id) {
		this.id=id;
	}
	public String getId() {
		return id;
	}
}
