package basicClass;

public class Book{ //extends Object 생략
	int bookNum;
	String bookTitle;
	
	Book(int bookNum, String bookTitle){
		this.bookNum = bookNum;
		this.bookTitle = bookTitle;
	}
	
	//메서드 재정의
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return super.toString();
	}
	

	
}
